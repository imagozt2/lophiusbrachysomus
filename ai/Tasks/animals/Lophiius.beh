<BehaviorSets>
<!--Wander behavior sets for randomized wandering-->
   <Wander>
      <subjects>
         <Lophiius/>
      </subjects>
      <behaviors>
         <BFBehLocoSwitchSet>
            <behaviorTable>
               <ground behSet="Idle_Ground"/>
               <water behSet="WanderSurf"/>
               <underwater behSet="WanderSub"/>
               <default behSet="WanderSurf"/>
            </behaviorTable>
         </BFBehLocoSwitchSet>
      </behaviors>
   </Wander>
   <WanderSurf>
      <subjects>
         <Lophiius/>
      </subjects>
      <behaviors>
         <BFBehPlaySet behSet="Surface"/>
         <ZTBehFeedback>
            <ZTFeedbackData>
               <ZTActionInfo locID="animalactions:SwimmingAround"/>
            </ZTFeedbackData>
         </ZTBehFeedback>
         <BFBehRandomSet>
            <randomSets>
               <WanderSurf_Fast weight="25"/>
               <WanderSurf_Med weight="25"/>
               <WanderSurf_Slow weight="25"/>
               <WanderSurf_Glide weight="25"/>
            </randomSets>
         </BFBehRandomSet>
      </behaviors>
   </WanderSurf>
   <WanderSurf_Fast>
      <subjects>
         <Lophiius/>
      </subjects>
      <behaviors>
         <BFBehWander minPlayTime="7" maxPlayTime="15" avoidLand="true" locoSpeed="fast" avoidEntityRadius="8">
            <avoidEntityTypes>
               <TrainingArea/>
            </avoidEntityTypes>
         </BFBehWander>
      </behaviors>
   </WanderSurf_Fast>
   <WanderSurf_Med>
      <subjects>
